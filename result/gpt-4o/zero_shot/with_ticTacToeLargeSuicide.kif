(<= (legal xPlayer (mark X Y))
    (true (control xPlayer))
    (true (cell X Y b)))
(<= (legal oPlayer (mark X Y))
    (true (control oPlayer))
    (true (cell X Y b)))
(<= (legal xPlayer noop)
    (true (control xPlayer))
    (not (exists (X Y) (true (cell X Y b)))))
(<= (legal oPlayer noop)
    (true (control oPlayer))
    (not (exists (X Y) (true (cell X Y b)))))

(<= (next (cell X Y x))
    (does xPlayer (mark X Y)))
(<= (next (cell X Y o))
    (does oPlayer (mark X Y)))
(<= (next (cell X Y V))
    (true (cell X Y V))
    (distinct V b)
    (not (does xPlayer (mark X Y)))
    (not (does oPlayer (mark X Y))))
(<= (next (control xPlayer))
    (true (control oPlayer)))
(<= (next (control oPlayer))
    (true (control xPlayer)))

(<= (line Player)
    (role Player)
    (or (row Player)
        (column Player)
        (diagonal Player)))

(<= (row Player)
    (role Player)
    (true (cell X 1 Player))
    (true (cell X 2 Player))
    (true (cell X 3 Player))
    (true (cell X 4 Player))
    (true (cell X 5 Player)))

(<= (column Player)
    (role Player)
    (true (cell 1 Y Player))
    (true (cell 2 Y Player))
    (true (cell 3 Y Player))
    (true (cell 4 Y Player))
    (true (cell 5 Y Player)))

(<= (diagonal Player)
    (role Player)
    (or (and (true (cell 1 1 Player))
             (true (cell 2 2 Player))
             (true (cell 3 3 Player))
             (true (cell 4 4 Player))
             (true (cell 5 5 Player)))
        (and (true (cell 1 5 Player))
             (true (cell 2 4 Player))
             (true (cell 3 3 Player))
             (true (cell 4 2 Player))
             (true (cell 5 1 Player)))))

(<= (full)
    (not (exists (X Y) (true (cell X Y b)))))

(<= terminal
    (line xPlayer))
(<= terminal
    (line oPlayer))
(<= terminal
    (full))

