(<= (legal player noop) true)
(<= (legal player (switch ?x ?y))
    (true (cell ?x ?y ?state))
    (<= (valid_coord ?x) (<= 1 ?x 5))
    (<= (valid_coord ?y) (<= 1 ?y 5)))

(<= (next (cell ?x ?y light))
    (does player (switch ?x ?y))
    (true (cell ?x ?y nolight)))
(<= (next (cell ?x ?y nolight))
    (does player (switch ?x ?y))
    (true (cell ?x ?y light)))

(<= (next (cell ?x ?y light))
    (does player (switch ?a ?b))
    (adjacent ?a ?b ?x ?y)
    (true (cell ?x ?y nolight)))
(<= (next (cell ?x ?y nolight))
    (does player (switch ?a ?b))
    (adjacent ?a ?b ?x ?y)
    (true (cell ?x ?y light)))

(<= (next (cell ?x ?y ?state))
    (does player noop)
    (true (cell ?x ?y ?state)))

(<= (next (step ?n))
    (true (step ?m))
    (succ ?m ?n))

(<= (adjacent ?x ?y ?a ?b)
    (or (and (succ ?x ?a) (= ?y ?b))
        (and (succ ?a ?x) (= ?y ?b))
        (and (succ ?y ?b) (= ?x ?a))
        (and (succ ?b ?y) (= ?x ?a))))

(<= (terminal)
    (true (step 20)))
(<= (terminal)
    (not (true (cell ?x ?y light))))

(<= (goal player 100)
    (not (true (cell ?x ?y light))))
(<= (goal player 0)
    (true (cell ?x ?y light)))

