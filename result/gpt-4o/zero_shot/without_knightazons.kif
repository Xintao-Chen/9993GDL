(<= (legal white noop) (true (control black)))
(<= (legal black noop) (true (control white)))

(<= (legal white (move X1 Y1 X2 Y2 AX AY))
    (true (control white))
    (true (cell X1 Y1 white))
    (knightmove X1 Y1 X2 Y2)
    (not (occupied X2 Y2))
    (knightmove X2 Y2 AX AY)
    (not (occupied AX AY)))

(<= (legal black (move X1 Y1 X2 Y2 AX AY))
    (true (control black))
    (true (cell X1 Y1 black))
    (knightmove X1 Y1 X2 Y2)
    (not (occupied X2 Y2))
    (knightmove X2 Y2 AX AY)
    (not (occupied AX AY)))

(<= (next (cell X2 Y2 P))
    (does P (move X1 Y1 X2 Y2 AX AY)))

(<= (next (cell AX AY arrow))
    (does P (move X1 Y1 X2 Y2 AX AY)))

(<= (next (cell X Y P))
    (true (cell X Y P))
    (not (does P (move X Y _ _ _ _))))

(<= (next (cell X Y arrow))
    (true (cell X Y arrow)))

(<= (next (control black))
    (true (control white)))

(<= (next (control white))
    (true (control black)))

(<= (occupied X Y)
    (true (cell X Y _)))

(<= (occupied X Y)
    (true (cell X Y arrow)))

(<= (knightmove X1 Y1 X2 Y2)
    (or (and (succ X1 X2) (succ Y1 Y3) (succ Y3 Y2))
        (and (succ X1 X3) (succ X3 X2) (succ Y1 Y2))
        (and (succ X2 X1) (succ Y1 Y3) (succ Y3 Y2))
        (and (succ X2 X3) (succ X3 X1) (succ Y1 Y2))
        (and (succ Y1 Y2) (succ X1 X3) (succ X3 X2))
        (and (succ Y1 Y3) (succ Y3 Y2) (succ X1 X2))
        (and (succ Y2 Y1) (succ X1 X3) (succ X3 X2))
        (and (succ Y2 Y3) (succ Y3 Y1) (succ X1 X2))))

(<= (terminal)
    (not (legal white _)))

(<= (terminal)
    (not (legal black _)))

