(<= (legal ?player finish)
    (true (control ?player)))

(<= (legal white continue)
    (true (control white)))

(<= (legal black continue)
    (true (control black)))

(<= (legal white noop)
    (true (control black)))

(<= (legal black noop)
    (true (control white)))

(<= (next (control black))
    (true (control white))
    (does white continue))

(<= (next (control white))
    (true (control black))
    (does black continue))

(<= (next (payoff white ?newPayoff))
    (true (control white))
    (does white continue)
    (true (payoff white ?currentPayoff))
    (succ ?currentPayoff ?newPayoff))

(<= (next (payoff black ?newPayoff))
    (true (control white))
    (does white continue)
    (true (payoff black ?currentPayoff))
    (succ ?currentPayoff ?intermediatePayoff)
    (succ ?intermediatePayoff ?newPayoff))

(<= (next (payoff black ?newPayoff))
    (true (control black))
    (does black continue)
    (true (payoff black ?currentPayoff))
    (succ ?currentPayoff ?newPayoff))

(<= (next (payoff white ?currentPayoff))
    (true (payoff white ?currentPayoff))
    (or (does black continue) (does black finish)))

(<= (next (payoff black ?currentPayoff))
    (true (payoff black ?currentPayoff))
    (or (does white continue) (does white finish)))

(<= (next (move ?nextMove))
    (true (move ?currentMove))
    (succ ?currentMove ?nextMove))

